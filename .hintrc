{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/strict": "off",
    "typescript-config/consistent-casing": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 108",
    "not chrome <= 108",
    "not edge <= 108",
    "not firefox <= 108",
    "not safari <= 16.2",
    "not samsung <= 19"
  ]
}